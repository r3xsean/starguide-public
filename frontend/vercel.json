{
  "rewrites": [
    { "source": "/character/:id", "destination": "/index.html" },
    { "source": "/best-teams", "destination": "/index.html" },
    { "source": "/pull-advisor", "destination": "/index.html" },
    { "source": "/banner-advisor", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
